{
  "name": "snowpack-plugin-copy",
  "version": "1.0.1",
  "description": "copy files plugin for snowpack",
  "keywords": [
    "snowpack",
    "plugin",
    "copy",
    "snowpack plugin",
    "snowpack plugin copy"
  ],
  "homepage": "https://github.com/donniean/snowpack-plugin-copy#readme",
  "bugs": {
    "url": "https://github.com/donniean/snowpack-plugin-copy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/snowpack-plugin-copy.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "main": "plugin.js",
  "files": [
    "plugin.js"
  ],
  "scripts": {
    "prettier": "npx prettier \"**/*.{js,json,md}\"",
    "eslint": "npx eslint --fix \"**/*.{js}\"",
    "cspell": "cspell \"**/*\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "pub": "npm publish --access public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cpy": "^8.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.3",
    "cspell": "^5.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  }
}
